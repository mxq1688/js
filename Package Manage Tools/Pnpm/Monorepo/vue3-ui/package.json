        "@vue/runtime-core": "^3.4.4",
        "commander": "^11.1.0",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^8.0.0",
        "gulp-less": "^5.0.0",
        "less": "^4.2.0",
        "release-it": "^17.0.1",
        "shelljs": "^0.8.5",
        "sucrase": "^3.35.0",
